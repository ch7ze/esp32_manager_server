    "module": "es6",
    "target": "es6",
    "sourceMap": true,
    "outDir": "./client/scripts",
    "rootDir": "./src",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}